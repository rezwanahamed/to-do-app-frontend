# .env.example

# API endpoint for the backend server
REACT_APP_API_URL=http://localhost:5000/

# Environment (development, production, etc.)
REACT_APP_ENV=development

# Port number for the frontend application
PORT=3000

# Secret key for JWT authentication (example value, replace with actual secret)
REACT_APP_JWT_SECRET=your_jwt_secret_key

# Google Analytics tracking ID (example value, replace with actual ID)
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X